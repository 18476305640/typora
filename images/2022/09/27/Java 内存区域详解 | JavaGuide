input[type="checkbox"].task-list-item-checkbox { position: relative; cursor: pointer; }

input[type="checkbox"].task-list-item-checkbox::after { content: " "; position: absolute; top: 0px; display: inline-block; box-sizing: border-box; width: 14px; height: 14px; padding-left: 0px; border: 1px solid rgb(221, 221, 221); border-radius: 2px; background-color: rgb(255, 255, 255); text-align: center; visibility: visible; }

html[data-theme="dark"] input[type="checkbox"].task-list-item-checkbox::after { border-color: rgb(102, 102, 102); background-color: rgb(51, 51, 51); }

input[type="checkbox"].task-list-item-checkbox:checked::after { content: ""; }

html[data-theme="dark"] input[type="checkbox"].task-list-item-checkbox:checked::after, input[type="checkbox"].task-list-item-checkbox:checked::after { border-color: var(--c-brand, #3eaf7c); background-color: var(--c-brand, #3eaf7c); }

input[type="checkbox"].task-list-item-checkbox:checked::before { content: ""; position: absolute; top: 1px; left: 5px; z-index: 1; width: 3px; height: 8px; border-top-color: ; border-top-style: ; border-right-color: ; border-right-style: ; border-bottom-color: ; border-bottom-style: ; border-left-color: ; border-left-style: ; border-image-source: ; border-image-slice: ; border-image-width: ; border-image-outset: ; border-image-repeat: ; border-width: 0px 2px 2px 0px; transform: rotate(45deg); }

.page.not-found { display: block; max-width: var(--home-page-width); min-height: 80vh; margin: 0px auto; text-align: center; padding: calc(var(--navbar-height) + 1rem) 2rem 0 !important; }

@media (max-width: 959px) {
  .page.not-found { padding-top: calc(var(--navbar-mobile-height) + 1rem); }
}

.page.not-found .page-404-icon { max-width: 600px; margin: 0px 1.5rem; }

html[data-theme="dark"] .page.not-found .page-404-icon { filter: invert(70%); }

.page.not-found .action-button { display: inline-block; box-sizing: border-box; margin: 0px 0.25rem; padding: 0.5rem 1rem; border-top-width: 0px; border-right-width: 0px; border-left-width: 0px; border-bottom: 1px solid var(--theme-color-dark); border-radius: 0.25rem; background: var(--theme-color); color: var(--white); outline: 0px; font-size: 1rem; transition: background 0.1s ease 0s; }

.page.not-found .action-button:hover { background: var(--theme-color-light); cursor: pointer; }

.skip-link { top: 0.25rem; left: 0.25rem; z-index: 999; padding: 0.65rem 1.5rem; border-radius: 0.5rem; background-color: var(--bg-color); box-shadow: var(--card-shadow); font-weight: 700; font-size: 0.9em; text-decoration: none; }

.skip-link, html[data-theme="dark"] .skip-link { color: var(--theme-color); }

.skip-link .skip-link:focus { clip: auto; width: auto; height: auto; clip-path: none; }
